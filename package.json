{
  "name": "elden-js",
  "version": "1.1.5",
  "description": "Simplified middleware library for JS/TS",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/backend-entry.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts src/backend-entry.ts --watch",
    "test": "echo \"No tests yet\" && exit 0"
  },
  "exports": {
    ".": "./dist/index.js",
    "./backend-entry": "./dist/backend-entry.js"
  },
  "keywords": [
    "elden",
    "auth",
    "auth control",
    "rate limiting",
    "jwt",
    "redis",
    "simplified",
    "frontend",
    "backend",
    "typescript",
    "js"
  ],
  "author": "Jebesh",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "ioredis": "^5.7.0"
  }
}
